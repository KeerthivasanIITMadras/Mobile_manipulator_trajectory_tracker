<?xml version="1.0"?>
<launch>
    <include file="$(find youbot_description)/launch/youbot_gazebo_world.launch" />   

    <rosparam file="$(find youbot_description)/controller/youbot_gazebo_controller.yaml" command="load"/>
    
    <node args="arm_joint_1_controller arm_joint_2_controller
    arm_joint_3_controller arm_joint_4_controller arm_joint_5_controller gripper_finger_joint_l_controller gripper_finger_joint_r_controller joint_state_controller " name="controller_spawner" ns="/youbot" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
    
    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
     	<remap from="/joint_states" to="/youbot/joint_states" />
    </node>

    
    
</launch>
